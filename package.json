{
  "name": "upauto",
  "version": "0.0.12",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "description": "autoupdate",
  "author": "Prateek",
  "build": {
    "appId": "com.autoupdate"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --win --publish=always",
    "build": "yarn react-build && electron-builder build --win --publish never",
    "deploy": "yarn react-build && electron-builder build --win --publish always",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "wait-on": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prateekkaistha/autoupdate.git"
  }
}
